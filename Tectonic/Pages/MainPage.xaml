<Page x:Class="PuzzleSolver.MainPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:PuzzleSolver"
      mc:Ignorable="d" 
      d:DesignHeight="600" d:DesignWidth="800"
      Title="MainPage">

    <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
        <Button Content="Create new puzzle" Width="150" Click="GoToPageOne_Click" Margin="40" />
        <Button Content="Go to puzzle List" Width="150" Click="GoToPageTwo_Click" />
    </StackPanel>
</Page>
